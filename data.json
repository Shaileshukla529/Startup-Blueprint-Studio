  "success_metrics": [
    {
      "category": "Financial Metrics",
      "metrics": [
        {
          "name": "Monthly Recurring Revenue (MRR)",
          "description": "Predictable revenue generated each month",
          "calculation": "Sum of all subscription revenues normalized to monthly",
          "target_growth": "15-20% month-over-month"
        },
        {
          "name": "Customer Acquisition Cost (CAC)",
          "description": "Cost to acquire a new customer",
          "calculation": "Total acquisition costs / Number of customers acquired",
          "benchmark": "Should be recovered within 12 months"
        },
        {
          "name": "Customer Lifetime Value (CLV)",
          "description": "Total revenue expected from a customer",
          "calculation": "Average revenue per customer / Churn rate",
          "target_ratio": "CLV:CAC ratio of 3:1 or higher"
        }
      ]
    },
    {
      "category": "Product Metrics",
      "metrics": [
        {
          "name": "Daily/Monthly Active Users",
          "description": "Users who engage with product regularly",
          "calculation": "Unique users performing core actions",
          "benchmark": "Varies by industry and product type"
        },
        {
          "name": "Feature Adoption Rate",
          "description": "Percentage of users adopting new features",
          "calculation": "Users using feature / Total users",
          "target": "30-40% adoption within 3 months"
        }
      ]
    }
  ]
